<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="root" prefHeight="375.0" prefWidth="640" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.edu.sustech.cs110.snake.control.HomeController">
    <AnchorPane prefHeight="383.0" prefWidth="658.0">
        <children>
            <Button fx:id="Begin" layoutX="30.0" layoutY="41.0" onAction="#playGame" prefHeight="106.0" prefWidth="289.0" text="new game">
                <font>
                    <Font name="Times New Roman" size="40.0" />
                </font>
            </Button>
            <Button fx:id="Load" layoutX="30.0" layoutY="208.0" mnemonicParsing="false" onAction="#loadGame" prefHeight="106.0" prefWidth="289.0" text="load game">
                <font>
                    <Font name="Times New Roman" size="40.0" />
                </font>
            </Button>
        </children>
    </AnchorPane>
</VBox>