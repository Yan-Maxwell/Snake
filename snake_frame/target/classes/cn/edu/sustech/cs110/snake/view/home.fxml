<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="root" prefHeight="375.0" prefWidth="421.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.edu.sustech.cs110.snake.control.HomeController">
    <AnchorPane prefHeight="375.0" prefWidth="333.0">
        <children>
            <Button fx:id="Begin" layoutX="103.0" layoutY="14.0" onAction="#playGame" prefHeight="106.0" prefWidth="225.0" text="new game">
                <font>
                    <Font name="Times New Roman" size="40.0" />
                </font>
            </Button>
            <Button fx:id="Load" layoutX="103.0" layoutY="135.0" mnemonicParsing="false" onAction="#loadGame" prefHeight="106.0" prefWidth="225.0" text="load game">
                <font>
                    <Font name="Times New Roman" size="40.0" />
                </font>
            </Button>
         <Button fx:id="ShowRank" layoutX="103.0" layoutY="255.0" onAction="#showranking" prefHeight="106.0" prefWidth="225.0" text="show rank">
            <font>
               <Font name="Times New Roman" size="40.0" />
            </font>
         </Button>
        </children>
    </AnchorPane>
</VBox>
